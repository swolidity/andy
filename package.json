    "@mdx-js/react": "^1.5.3",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.18.8",
    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-source-filesystem": "^2.1.43",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "tailwindcss": "^1.1.4"
  },
  "repository": {
